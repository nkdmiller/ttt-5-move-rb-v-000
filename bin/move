#!/usr/bin/env ruby

require_relative '../lib/move.rb'

puts "Hello welcome to Tic Tac Toe!"

board = [" "," "," "," "," "," "," "," "," "]

display_board(board)

puts "Where would you like to go?"

input = gets.strip

input_to_index(input)

move(board,index,character="X")

display_board(board)
